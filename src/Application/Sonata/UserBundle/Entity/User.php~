class User extends BaseUser
{

    public function __construct()
    {
        parent::__construct();
        $this->enabled = true;
        $this->dateOfBirth = new \DateTime('1950-01-01');
    }

    public function __toString()
    {
        return (string) $this->username.' - '.$this->firstname.' '.$this->lastname;
    }

